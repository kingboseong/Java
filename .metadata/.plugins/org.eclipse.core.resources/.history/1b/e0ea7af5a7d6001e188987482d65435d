package Qize;

//Geometry부모 클래스를 다른 자식 클래스에게 상속함.
class Geometry {
}

class Circle extends Geometry {
	int radius;
}

class Rectangle extends Geometry {
	int width;
	int height;
}

class Triangle extends Geometry {
	int width;
	int height;
}

